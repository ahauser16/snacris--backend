{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/realPropertyMasterNew.schema.json",
  "type": "object",
  "properties": {
    "document_id": { "type": "string", "minLength": 16, "maxLength": 16 },
    "record_type": {
      "type": "string",
      "enum": ["A"]
    },
    "crfn": { "type": "string", "minLength": 13, "maxLength": 13 },
    "recorded_borough": {
      "type": "integer",
      "enum": [1, 2, 3, 4, 5]
    },
    "doc_type": {
      "type": "string",
      "minLength": 1,
      "maxLength": 8
    },
    "document_date": { "type": "string", "format": "date" },
    "document_amt": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 999999999999.99,
      "multipleOf": 0.01
    },
    "recorded_datetime": { "type": "string", "format": "date-time" },
    "modified_date": { "type": "string", "format": "date" },
    "reel_yr": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "reel_nbr": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "reel_pg": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99999
    },
    "percent_trans": {
      "type": "number",
      "maximum": 100,
      "minimum": 0,
      "multipleOf": 0.000001
    },
    "good_through_date": { "type": "string", "format": "date" }
  },
  "additionalProperties": false,
  "required": [
    "document_id",
    "record_type",
    "crfn",
    "recorded_borough",
    "doc_type",
    "document_date",
    "document_amt",
    "recorded_datetime",
    "modified_date",
    "reel_yr",
    "reel_nbr",
    "reel_pg",
    "percent_trans",
    "good_through_date"
  ]
}
